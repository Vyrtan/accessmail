#:kivy 1.7.2

<ReadLayout>:
    mn: men
    BoxLayout:
        orientation: "horizontal"
        MenuLayout:
            id: men
            size_hint: [0.2,1]
        BoxLayout:
            orientation: "vertical"
            GridLayout:
                size_hint: [1, 0.1]
                rows: 1
                Button:
                    text: "Previous"
                Button:
                    text: "Save"
                Button:
                    text: "Next"
            Label:
                text: "Output"